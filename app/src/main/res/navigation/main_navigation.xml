<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_navigation"
    app:startDestination="@+id/navigation_map">

    <fragment
        android:id="@+id/navigation_map"
        android:name="com.stellarworker.geonote.ui.maps.MapsFragment"
        android:label="@string/bottom_navigation_map"
        tools:layout="@layout/fragment_maps" />

    <fragment
        android:id="@+id/navigation_markers"
        android:name="com.stellarworker.geonote.ui.markers.MarkersFragment"
        android:label="@string/bottom_navigation_markers"
        tools:layout="@layout/fragment_markers" />

</navigation>